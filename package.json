{
  "name": "yt-on-yt",
  "version": "0.0.0",
  "displayName": "YT on YT",
  "description": "yt-on-yt",
  "private": true,
  "sideEffects": false,
  "engines": {
    "node": "18.18.2"
  },
  "volta": {
    "node": "18.18.2"
  },
  "scripts": {
    "publish:chrome": "node scripts/publish/chrome.mjs",
    "publish:edge": "node scripts/publish/edge.mjs",
    "publish": "concurrently -n \"chrome,edge\" -c \"red.bold,blue.bold\" \"npm:publish:chrome\" \"npm:publish:edge\"",
    "clean": "rimraf dist",
    "prepare": "husky install",
    "compress": "node scripts/compress.mjs",
    "dev": "concurrently -n \"watch,hmr\" -c \"red.green,blue.yellow\" \"npm:watch:chrome\" \"npm:watch:dist\"",
    "build": "yarn clean && cross-env NODE_ENV=production webpack && yarn compress",
    "build:stats": "yarn clean && cross-env STATS=1 NODE_ENV=production webpack",
    "build:local": "yarn clean && cross-env NODE_ENV=production webpack && yarn compress",
    "build:chrome": "cross-env NODE_ENV=production webpack --config ./webpack/chrome.js",
    "build:firefox": "cross-env NODE_ENV=production webpack --config ./webpack/firefox.js",
    "watch": "yarn clean && cross-env NODE_ENV=development webpack -w",
    "watch:chrome": "cross-env NODE_ENV=development webpack --config ./webpack/chrome.js -w",
    "watch:firefox": "cross-env NODE_ENV=development webpack --config ./webpack/firefox.js -w",
    "watch:dist": "mv3-hot-reload",
    "dev:chrome": "concurrently -n \"build,mv3\" -c \"red.bold,blue.bold\" \"yarn watch:chrome\" \"yarn watch:dist\"",
    "start": "yarn clean && concurrently -n \"chrome,firefox\" -c \"red.bold,blue.bold\" \"npm:watch:chrome\" \"npm:watch:firefox\"",
    "pretty": "prettier --loglevel=error --write .",
    "postinstall": "patch-package"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,html,css,json,md}": "prettier --loglevel=error --write"
  },
  "dependencies": {
    "@emotion/react": "^11.10.8",
    "@emotion/styled": "^11.10.8",
    "@mantine/core": "^6.0.9",
    "@mantine/hooks": "^6.0.9",
    "@mantine/notifications": "^6.0.9",
    "@sentry/browser": "^7.57.0",
    "@sentry/integrations": "^7.50.0",
    "@sentry/webpack-plugin": "^2.4.0",
    "@tabler/icons": "2.17.0",
    "@tabler/icons-react": "^2.17.0",
    "emotion": "^11.0.0",
    "fix-webm-duration": "^1.0.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remeda": "^1.14.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.5",
    "@babel/core": "^7.21.5",
    "@babel/plugin-transform-react-jsx": "^7.21.5",
    "@babel/preset-env": "^7.21.5",
    "@emotion/babel-plugin": "^11.10.8",
    "@plasmohq/edge-addons-api": "^1.2.0",
    "@types/chrome": "^0.0.234",
    "@types/node": "^20.4.5",
    "@types/react-dom": "^18.2.1",
    "@types/webextension-polyfill": "^0.10.0",
    "archiver": "^5.3.1",
    "babel-loader": "^9.1.2",
    "chrome-webstore-upload": "^1.0.0",
    "concurrently": "^8.0.1",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.3",
    "css-minimizer-webpack-plugin": "^5.0.0",
    "dotenv": "^16.0.3",
    "dotenv-expand": "^10.0.0",
    "generate-json-from-js-webpack-plugin": "^0.1.1",
    "html-minimizer-webpack-plugin": "^4.3.0",
    "html-webpack-plugin": "^5.5.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "mini-css-extract-plugin": "^2.7.5",
    "mkdirp": "^3.0.1",
    "mv3-hot-reload": "^0.2.7",
    "node-args": "^2.1.8",
    "patch-package": "^7.0.0",
    "path": "^0.12.7",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.0",
    "style-loader": "^3.3.2",
    "terser-webpack-plugin": "^5.3.7",
    "ts-loader": "^9.4.2",
    "tsconfig-paths-webpack-plugin": "^4.0.1",
    "typescript": "^5.0.4",
    "webextension-polyfill": "^0.10.0",
    "webpack": "^5.81.0",
    "webpack-bundle-analyzer": "^4.8.0",
    "webpack-cli": "^5.0.2",
    "webpack-dev-server": "^4.13.3",
    "webpack-merge": "^5.8.0",
    "webpack-shell-plugin-next": "^2.3.1"
  }
}
